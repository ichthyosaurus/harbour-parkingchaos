[Desktop Entry]
Type=Application
X-Nemo-Application-Type=silica-qt5
Icon=harbour-parkingchaos
Exec=harbour-parkingchaos

# translation example:
# your app name in German locale (de)
#
# Remember to comment out the following line, if you do not want to use
# a different app name in German locale (de).
Name=Parking Chaos
Name[de]=Parkplatzchaos
Name[en]=Parking Chaos
Name[sv]=Parkeringskaos

[X-Sailjail]
Permissions=
OrganizationName=harbour-parkingchaos
ApplicationName=harbour-parkingchaos

[X-HarbourBackup]
BackupConfigList=/apps/harbour-parkingchaos/
